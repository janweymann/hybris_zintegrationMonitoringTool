    <widget-extension widgetId="integrationTypeCodeRouter"
                      widgetDefinitionId="com.hybris.cockpitng.widgets.common.multiplexer"
                      slotId="cockpitWidgetChildrenInvisible">
        <setting key="errorMonitoringRequest_expression" type="String">#root eq 'ErrorMonitoringRequest'</setting>
        <virtual-sockets>
            <output id="errorMonitoringRequest" type="java.lang.String"/>
        </virtual-sockets>
    </widget-extension>

    <widget-extension widgetId="integrationBackofficeViewSwitcher">
        <widget id="errorMonitor" widgetDefinitionId="com.hybris.cockpitng.collapsiblecontainer"
            slotId="views" title="Error Monitoring">
            <widget id="errorMonitoringRequestSearchEngine" widgetDefinitionId="com.hybris.cockpitng.widgets.common.advancedsearchengine"
                    slotId="cockpitWidgetChildrenInvisible">
                <virtual-sockets/>
            </widget>
            <widget id="errorMonitoringRequestSearchCondition" widgetDefinitionId="com.hybris.cockpitng.advancedsearch"
                    slotId="top">
                <setting key="disableSubtypesCheckbox" type="Boolean">true</setting>
                <setting key="disableAdvancedSearchToolbar" type="Boolean">true</setting>
                <setting key="isNestedObjectCreationDisabled" type="Boolean">true</setting>
                <virtual-sockets />
            </widget>
            <widget id="errorMonitoringRequestSearchResult"
                    widgetDefinitionId="com.hybris.cockpitng.collectionBrowser"
                    slotId="center">
                <setting key="socketDataType_$T" type="String">
                    de.hybris.platform.inboundservices.model.InboundRequestModel</setting>
            </widget>
            <widget id="errorMonitoringRequestDetails"
                    widgetDefinitionId="com.hybris.cockpitng.backoffice.defaultEditorArea"
                    slotId="bottom">
                <setting key="socketDataType_$T" type="String">
                    de.hybris.platform.inboundservices.model.InboundRequestModel
                </setting>
                <setting key="toolbarDisabled" type="Boolean">true</setting>
                <setting key="editorAreaConfigCtx" type="String">integration-editor-area</setting>
            </widget>
            <setting key="topPanelCollapsed">true</setting>
        </widget>
        <setting key="selectErrorMonitor_eval" type="String" value="'errorMonitor'" />
        <virtual-sockets>
            <input id="selectErrorMonitor" type="java.lang.Object" />
        </virtual-sockets>
    </widget-extension>

    <widget-connection sourceWidgetId="integrationTypeCodeRouter" outputId="errorMonitoringRequest"
                       targetWidgetId="integrationBackofficeViewSwitcher" inputId="selectErrorMonitor" />
    <widget-connection sourceWidgetId="integrationTypeCodeRouter" outputId="errorMonitoringRequest"
                       targetWidgetId="errorMonitor" inputId="activate" />
    <widget-connection sourceWidgetId="integrationTypeCodeRouter" outputId="errorMonitoringRequest"
                       targetWidgetId="errorMonitoringRequestSearchCondition" inputId="type" />
    <widget-connection sourceWidgetId="errorMonitoringRequestSearchCondition" outputId="searchData"
                       targetWidgetId="errorMonitoringRequestSearchEngine" inputId="searchData" />
    <widget-connection sourceWidgetId="errorMonitoringRequestSearchEngine" outputId="pageable"
                       targetWidgetId="errorMonitoringRequestSearchResult" inputId="pageable" />
    <widget-connection sourceWidgetId="errorMonitoringRequestSearchResult" outputId="selectedItem"
                       targetWidgetId="errorMonitoringRequestDetails" inputId="inputObject" />

