    <widget-extension widgetId="integrationTypeCodeExtractor">
        <widget id="integrationTypeCodeRouterExtended"
                widgetDefinitionId="com.hybris.cockpitng.widgets.common.multiplexer">
        <setting key="errorMonitoring_expression" type="String">#root eq 'errorMonitoring'</setting>
        <virtual-sockets>
            <output id="errorMonitoring" type="java.lang.String"/>
        </virtual-sockets>
        </widget>
    </widget-extension>

    <widget-connection sourceWidgetId="integrationTypeCodeRouter" outputId="errorMonitoring"
    targetWidgetId="integrationBackofficeViewSwitcher" inputId="selectInboundChannelConfigurationContainer" />

    <widget-connection sourceWidgetId="integrationTypeCodeRouter" outputId="errorMonitoring"
                       targetWidgetId="inboundChannelConfigurationContainer" inputId="activate"/>







